# 列表
my_list = [1, 2, "哈哈", 1.3]
# print(my_list)
# print(my_list[2])
# print(my_list[-4])

# 列表增加元素：append()  insert()  extend()
my_list.append("1111")
my_list.insert(2, "刘雨昕")
my_list.extend("陆柯燃")
print(my_list)

# 列表删除元素：pop()  renmove()
my_list.pop(1)
my_list.remove('1111')
my_list.remove(1.3)
print(my_list)

# 列表的修改
my_list[0] = 33
print(my_list)

# 元组（不可变的列表）()
my_list2 = (1, 2, "刘雨昕", 1.3)
print(my_list2[2])
